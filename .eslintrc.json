{
	"extends": [
	"eslint: recommended",
	"plugin:import/errors",
	"plugin:react/recommended",
	"plugin:jsx-a11y/recommended",
	"eslint:recommended",
	"prettier",
	"prettier/react"
	],
	"rules": {
		"react/props-types": 0
	},
	"plugin":["react", "import", "jsx-a11y"],
	"parserOptions": {
	"ecmaVersion": 2018,
	"sourceType": "modules",
	"ecmaFeatures": {
		"jsx": true
		}
	},
	"env": {
	"es6": true,
	"browser": true,
	"node": true
	}
}
